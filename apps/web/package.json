{
  "name": "@vnphanquang/web",
  "version": "0.0.1",
  "description": "frontend for vnphanquang",
  "private": "true",
  "author": "Quang Phan",
  "scripts": {
    "dev": "cross-env NODE_ENV=development svelte-kit dev",
    "build": "cross-env NODE_ENV=development svelte-kit build",
    "build:production": "cross-env NODE_ENV=production svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --check --ignore-path .gitignore --plugin-search-dir=. \"./**/*.{ts,js,cjs,json,svelte}\"",
    "codegen:routing": "roullector collect --inDir=\"./src/routes/[locale=locale]\"",
    "codegen": "run-p codegen:*"
  },
  "devDependencies": {
    "@icons-pack/svelte-simple-icons": "^2.3.0",
    "@svelte-put/clickoutside": "^1.0.0",
    "@svelte-put/intersect": "^1.0.2",
    "@svelte-put/movable": "^1.1.6",
    "@svelte-put/shortcut": "^1.0.0",
    "@sveltejs/adapter-vercel": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/typography": "^0.5.2",
    "@types/gtag.js": "^0.0.10",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vnphanquang/eslint-config": "workspace:^*",
    "@vnphanquang/prettierrc": "workspace:^",
    "@vnphanquang/tailwind": "workspace:^",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.9",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "highlight.js": "^11.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-load-config": "^4.0.0",
    "postcss-nesting": "^10.1.7",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "roullector": "^1.1.2",
    "svelte": "^3.48.0",
    "svelte-awesome": "^3.0.0",
    "svelte-check": "^2.7.1",
    "svelte-highlight": "^6.0.1",
    "svelte-preprocess": "^4.10.6",
    "sveltekit-i18n": "^2.2.2",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "typescript": "~4.6.4"
  },
  "type": "module",
  "dependencies": {
    "classnames": "^2.3.1",
    "fuse.js": "^6.6.2",
    "lodash.debounce": "^4.0.8"
  }
}
