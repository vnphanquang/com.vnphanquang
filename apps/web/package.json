{
  "name": "@vnphanquang/web",
  "version": "0.0.1",
  "description": "frontend for vnphanquang",
  "author": "Quang Phan",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite dev",
    "build": "cross-env NODE_ENV=development vite build",
    "build:production": "cross-env NODE_ENV=production vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --check --ignore-path .gitignore --plugin-search-dir=. \"./**/*.{ts,js,cjs,json,svelte}\"",
    "codegen:routing": "roullector collect --inDir=\"./src/routes/[locale=locale]\"",
    "codegen": "run-p codegen:*"
  },
  "devDependencies": {
    "@icons-pack/svelte-simple-icons": "^2.3.0",
    "@svelte-put/clickoutside": "^1.0.0",
    "@svelte-put/intersect": "^1.0.2",
    "@svelte-put/movable": "^1.1.6",
    "@svelte-put/shortcut": "^1.0.0",
    "@sveltejs/adapter-vercel": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/typography": "^0.5.2",
    "@types/cookie": "^0.5.1",
    "@types/d3-scale": "^4.0.2",
    "@types/gtag.js": "^0.0.10",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "@vnphanquang/eslint-config": "workspace:^0.0.0",
    "@vnphanquang/prettierrc": "workspace:^0.0.0",
    "@vnphanquang/tailwind": "workspace:^0.0.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.12",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "graphql": "^16.5.0",
    "highlight.js": "^11.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-nesting": "^10.1.10",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "roullector": "^1.1.2",
    "svelte": "^3.48.0",
    "svelte-awesome": "^3.0.0",
    "svelte-check": "^2.8.0",
    "svelte-highlight": "^6.1.2",
    "svelte-preprocess": "^4.10.7",
    "sveltekit-i18n": "^2.2.2",
    "tailwindcss": "^3.1.4",
    "tslib": "^2.4.0",
    "typescript": "~4.6.4",
    "vite": "^2.9.13"
  },
  "type": "module",
  "dependencies": {
    "@leveluptuts/g-query": "^0.2.3",
    "@popperjs/core": "^2.11.5",
    "classnames": "^2.3.1",
    "cookie": "^0.5.0",
    "d3-scale": "^4.0.2",
    "fuse.js": "^6.6.2",
    "graphql-tag": "^2.12.6",
    "jwt-decode": "^3.1.2",
    "lodash.debounce": "^4.0.8"
  }
}
