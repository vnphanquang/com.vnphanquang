{
  "name": "@vnphanquang/api",
  "version": "0.0.1",
  "description": "api for vnphanquang",
  "author": "Quang Phan",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --check --ignore-path .gitignore --plugin-search-dir=. \"./**/*.{ts,js,cjs,json}\"",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.16",
    "@nestjs/mercurius": "^10.0.16",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-fastify": "^8.4.7",
    "@prisma/client": "^3.15.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify": "^3.25.0",
    "graphql": "^16.5.0",
    "mercurius": "^9",
    "module-alias": "^2.2.2",
    "nest-typed-config": "^2.4.1",
    "passport": "^0.6.0",
    "passport-google-oauth": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@jest/types": "^28.1.1",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/jest": "27.5.0",
    "@types/node": "^16.0.0",
    "@types/passport-google-oauth": "^1.0.42",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vnphanquang/eslint-config": "workspace:^*",
    "@vnphanquang/prettierrc": "workspace:^",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "prisma": "^3.15.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "_moduleAliases": {
    "$config": "dist/config"
  }
}
